#include <iostream>
using namespace std;
int main() 
{
   // declaring variables and assingments
   double celcius,fahrenheit;
   cout << "Enter degree Celcius: \n";
   cin >> celcius;
   fahrenheit = (9/5*celcius) + 32;
   cout << "Degree Celcius: " << celcius;
   cout << " \n Degree Farenheit: " << fahrenheit;

}